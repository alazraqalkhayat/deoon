<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activities.DataActivity">


    <LinearLayout
        style="@style/branshes_container_linear_layout"
        android:background="@color/continer_black_color"
        >

        <Button
            style="@style/branshes_buttons"
            android:background="@drawable/new_shape_of_brunch_buttons"
            android:text="رفع البيانات"
            android:id="@+id/data_activity_export_data_button"

            />

    </LinearLayout>

    <LinearLayout
        style="@style/branshes_container_linear_layout"
        android:background="@color/continer_black_color"

        >

        <Button
            style="@style/branshes_buttons"
            android:background="@drawable/new_shape_of_brunch_buttons"
            android:text="إسترجاع البيانات"
            android:id="@+id/data_activity_import_data_button"


            />

    </LinearLayout>


</LinearLayout>